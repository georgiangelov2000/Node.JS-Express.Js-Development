<% for(let i=0; i < users.length; i++) { %>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].firstname %></td>
    <td><%= users[i].lastname %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].country %></td>
    <td><%= users[i].city %></td>
    <td>
      <a href="/update-user?id=<%= users[i]._id%>" class="btn btn-primary">
        <span>Update</span>
      </a>
      <a class="btn btn-primary text-white delete" data-id=<%= users[i]._id%> >
        <span>Delete</span>
      </a>
    </td>
  </tr>

<% } %>