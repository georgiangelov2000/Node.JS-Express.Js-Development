<% for(let i=0; i < users.length; i++) { %>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].firstname %></td>
    <td><%= users[i].lastname %></td>
    <td><%= users[i].country %></td>
    <td><%= users[i].city %></td>
    <td>
      <a href="/update-user?id=<%= users[i].id%>" class="btn btn-primary">
        <span>Update</span>
      </a>
      <a href="#" class="btn btn-primary">
        <span>Delete</span>
      </a>
    </td>
  </tr>

<% } %>